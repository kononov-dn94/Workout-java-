Дописать отсутствующие фрагменты кода 

class PoolPuzzleOne {
	public static void main (String [] args) {
		int x = 0;

		while (_____) {
			____________________
			if (x < 1) {
				_________________
			}
			____________________
			if (x > 1) {
				______________________
				x = x + 2;
			}
			if (x == 1) {
				________________________
			}
			if (x < 1) {
				____________________	
			}
			System.out.print("");
